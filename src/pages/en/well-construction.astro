---
import Container from '@/components/Container.astro'
import BaseLayout from '@/layouts/BaseLayout.astro'
import AboutSteps from '@/components/AboutSteps.astro'
import AboutUsHero from '@/components/AboutUsHero.astro'
import PartnersApp from '@/components/PartnersApp.astro'
import MainPage from '@/components/layout/MainPage.astro'
import PartnersCompany from '@/components/PartnersCompany.astro'
import PartnersPush from '@/components/PartnersPush.astro'

export const prerender = true
---

<BaseLayout
  title="Будівництво водних свердловин у Києві та Київській області! Надаємо професійні та надійні послуги з технічного обслуговування. | Безкоштовна консультація по вашому технічному завданню!"
  description="Компанія «AquaPumpers» - спеціалізується на монтажі насосів та насосних станцій у водну свердловину. Працюємо швидко, якісно та ефективно незалежно від обраної моделі насоса, глибини свердловини та інших її особливостей. Приймаємо замовлення у Києві та Київській області. Замовляйте зараз, щоб отримати ефективні та доступні рішення!"
>
  <MainPage />
  <Container>
    <AboutSteps />
    <PartnersApp />
    <AboutUsHero />
    <PartnersCompany />
    <PartnersPush />
  </Container>
</BaseLayout>
